<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="robot">
    <xacro:include filename="$(find robot_description)/urdf/four_wheel.gazebo"/>
    <pose>0 0 0 0 0 0</pose>
    <link name="robot_footprint"/>
    <link name="chassis">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value="5.0"/>
            <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
        </inertial>
        <visual name="">
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <box size="0.5 0.31 0.15"/>
            </geometry>
            <material name="">
                <color rgba="0.078 0.047 0.44137 1.0"/>
                <texture filename=""/>
            </material>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <box size="0.5 0.31 0.15"/>
            </geometry>
        </collision>
    </link>

    <joint name="joint_chassis" type="fixed">
        <parent link="robot_footprint"/>
        <child link="chassis"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
    </joint>

    <link name="right_front_wheel">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value="0.1"/>
            <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
        </inertial>
        <visual name="">
            <origin xyz="0.0 0.0 0.0" rpy="1.5707 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.08" length="0.05"/>
            </geometry>
            <material name="">
                <color rgba="0.1 0.1 0.1 1.0"/>
                <texture filename=""/>
            </material>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="1.5707 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.08" length="0.05"/>
            </geometry>
        </collision>
    </link>

    <joint name="right_front_wheel_joint" type="continuous">
        <origin xyz="0.15 -0.155 -0.06" rpy="0.0 0.0 0.0"/>
        <parent link="chassis"/>
        <child link="right_front_wheel"/>
        <axis xyz="0.0 1.0 0.0"/>
    </joint>

    <link name="left_front_wheel">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value="0.1"/>
            <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
