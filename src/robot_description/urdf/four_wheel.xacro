        <parent link="chassis"/>
        <child link="left_rear_wheel"/>
        <axis xyz="0.0 1.0 0.0"/>
    </joint>

    <link name="camera">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value="0.05"/>
            <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
        </inertial>
        <visual name="">
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <box size="0.04 0.13 0.05"/>
            </geometry>
